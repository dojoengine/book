"    #[key]\n"
"    x: u32,\n"
"    #[key]\n"
"    y: u32,\n"
"    piece: PieceType,\n"
"}\n"
"\n"
"#[derive(Serde, Drop, Copy, PartialEq, Introspect)]\n"
"enum PieceType {\n"
"    WhitePawn: (),\n"
"    WhiteKnight: (),\n"
"    WhiteBishop: (),\n"
"    WhiteRook: (),\n"
"    WhiteQueen: (),\n"
"    WhiteKing: (),\n"
"    BlackPawn: (),\n"
"    BlackKnight: (),\n"
"    BlackBishop: (),\n"
"    BlackRook: (),\n"
"    BlackQueen: (),\n"
"    BlackKing: (),\n"
"    None: (),\n"
"}\n"
"\n"
"#[derive(Serde, Drop, Copy, PartialEq, Introspect)]\n"
"enum Color {\n"
