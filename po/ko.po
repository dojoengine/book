"Moves));\n"
"    moves.remaining -= 1;\n"
"\n"
"    let next = next_position(position, direction);\n"
"    \n"
"    set !(ctx.world, (moves, next));\n"
"    emit !(ctx.world, Moved { address: ctx.origin, direction });\n"
"    return ();\n"
"}\n"
"```"

